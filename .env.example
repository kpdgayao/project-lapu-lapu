# Project Lapu-lapu Environment Variables
# Copy this file to .env and fill in your values

# Server
PORT=3000
NODE_ENV=development

# Retell AI
# Get your API key from: https://dashboard.retellai.com/
RETELL_API_KEY=your_retell_api_key_here

# Telnyx (Voice/SMS)
# Get your credentials from: https://portal.telnyx.com/
TELNYX_API_KEY=your_telnyx_api_key_here
TELNYX_PUBLIC_KEY=your_telnyx_public_key_here
TELNYX_PHONE_NUMBER=+63xxxxxxxxxx

# Semaphore SMS (Philippines)
# Get your API key from: https://semaphore.co/
SEMAPHORE_API_KEY=your_semaphore_api_key_here
SEMAPHORE_SENDER_NAME=PharmaCo

# Resend (Email)
# Get your API key from: https://resend.com/
RESEND_API_KEY=your_resend_api_key_here
RESEND_FROM_EMAIL=noreply@yourcompany.com

# Database (Phase 1+)
DATABASE_URL=postgresql://user:password@localhost:5432/lapulapu

# Dashboard Auth (Phase 4+)
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3001
